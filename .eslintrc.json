{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "max-len": [
            "error",
            {
                "code": 110,
                "ignoreComments": true,
                "ignoreStrings": true
            }
        ]
    }
}